package Array;

import java.util.Scanner;

public class SwapAlternate {
	
	public static void main(String[] args) {
		
		Scanner s=new Scanner(System.in);
		
		int size=s.nextInt();
		int arr[]=new int[size];
		
		for(int i=0;i<=arr.length;i+2) {
			
			int a=arr[i];
			int b=arr[i+1];
			
			arr[i]=b;
			arr[i+1]=a;
			
		}
		System.out.println(arr);
		
	}	
}


