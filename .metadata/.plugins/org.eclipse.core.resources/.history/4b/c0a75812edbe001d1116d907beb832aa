package Array;
import java.util.Arrays;
import java.util.Scanner;

public class ArrangeNumber {
	 public static void main(String args[]) {
		 Scanner s=new Scanner(System.in);
		 
		 int n=s.nextInt();
		 int arr[] =new int[n];
		 for(int i=0;i<n;i++) {
			 arr[i]=s.nextInt();
			
		 }
		 System.out.println(Arrays.toString(arr));
		 for(int i=0;i<n-1;i++) {
			 
			 int temp= arr[i];
			 int j=arr[i+1];
			 if(j<i) {
				 arr[i]=j;
//				 arr[i+1]=temp;
			 }
			 
		 }
		 System.out.println(Arrays.toString(arr));
		 
	 }

}
